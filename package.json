{
  "name": "vuegridle",
  "version": "1.0.2",
  "type": "module",
  "description": "Ultra-light (4.7kB gzip) grid library for Vue 3, powered by TypeScript & Composition API",
  "keywords": [
    "vue",
    "vue3",
    "grid",
    "draggable",
    "resizable",
    "typescript",
    "composition-api",
    "lightweight"
  ],
  "author": "Artem Doroshko <artemdoroshko555@gmail.com>",
  "license": "MIT",
  "main": "dist/vuegridle.cjs.js",
  "module": "dist/vuegridle.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vuegridle.es.js",
      "require": "./dist/vuegridle.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.20.1",
    "flexsearch": "^0.7.43",
    "prettier": "^3.5.1",
    "standard-version": "^9.5.0",
    "terser": "^5.39.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.3",
    "vitepress-plugin-search": "^1.0.4-alpha.22",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FrankieSR/VueGridle.git"
  }
}
